		"react-dom": "^19.0.0",
		"react-i18next": "^15.2.0",
		"react-image-crop": "^11.0.7",
		"react-markdown": "^9.0.1",
		"react-modal-hook": "^3.0.0",
		"react-redux": "^9.2.0",
		"react-router-dom": "^7.1.1",
		"react-twemoji": "^0.6.0",
		"reflect-metadata": "^0.2.2",
		"rndstr": "^1.0.0",
		"routing-controllers": "^0.10.1",
		"sass": "^1.38.2",
		"sass-loader": "^16.0.4",
		"striptags": "^3.2.0",
