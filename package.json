{
	"name": "misskey-tools",
	"version": "3.3.1-lqvp",
	"description": "",
	"main": "built/app.js",
	"author": "Shrimpia Network + lqvp",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "run-s build:backend build:frontend",
		"build:frontend": "webpack",
		"build:backend": "run-s build:backend-source build:views build:meta build:styles build:assets",
		"build:backend-source": "tsc",
		"build:views": "copyfiles -u 1 src/backend/views/*.pug ./built/",
		"build:assets": "copyfiles -u 1 assets/* ./built/assets/",
		"build:meta": "node ./build-meta.js",
		"build:styles": "sass styles/:built/assets",
		"start": "node built/app.js",
		"dev": "run-p 'dev:*'",
		"dev:backend": "nodemon",
		"dev:frontend": "webpack --watch",
		"clean": "rimraf built",
		"tsc": "tsc",
		"lint": "eslint --ext .ts,.tsx src",
		"lint:fix": "eslint --fix --ext .ts,.tsx src",
		"typeorm": "typeorm-ts-node-commonjs",
		"build:migrations": "tsc -p tsconfig.migrations.json && node rename-migrations.cjs",
		"migration:run": "yarn build:migrations && typeorm-ts-node-commonjs migration:run -d data-source.cjs",
		"migrate": "yarn migration:run",
		"migrate:revert": "ts-node --project ./tsconfig.migration.json ./node_modules/typeorm/cli.js migration:revert"
	},
	"dependencies": {
		"@babel/preset-react": "^7.14.5",
		"@koa/multer": "^3.0.2",
		"@reduxjs/toolkit": "^2.5.0",
		"axios": "^1.7.8",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.0",
		"css-loader": "^7.1.2",
		"dayjs": "^1.10.7",
		"deepmerge": "^4.2.2",
		"i18next": "^24.2.0",
		"i18next-browser-languagedetector": "^8.0.2",
		"insert-text-at-cursor": "^0.3.0",
		"json5-loader": "^4.0.1",
		"koa": "^2.13.0",
		"koa-bodyparser": "^4.3.0",
		"koa-router": "^13.0.1",
		"koa-send": "^5.0.1",
		"koa-session": "^6.0.0",
		"koa-views": "^8.1.0",
		"markdown-it": "^14.1.0",
		"misskey-js": "^2024.11.1-alpha.0",
		"ms": "^2.1.3",
		"node-cron": "^3.0.3",
		"object.pick": "^1.3.0",
		"pg": "^8.3.0",
		"pug": "^3.0.0",
